  fmiVersion="2.0"
  modelName="$modelName"
  guid="$guid"
  description=""
  generationTool="txtUML"
  generationDateAndTime="$generateDateAndTime"
  variableNamingConvention="structured"
  numberOfEventIndicators="0">
